<script>
  const artikelContents = document.querySelectorAll('.artikel-content');

  artikelContents.forEach(artikelContent => {
    const combinedParagraph = artikelContent.querySelector('.combined-paragraph');
    if (combinedParagraph) {
      let combinedText = '';

      const paragraphs = artikelContent.querySelectorAll('p');
      paragraphs.forEach(paragraph => {
        combinedText += paragraph.textContent + ' ';
      });

      if (combinedText.length > 200) {
        combinedText = combinedText.substring(0, 200) + '...';
      }

      combinedParagraph.textContent = combinedText;
    }
  });
