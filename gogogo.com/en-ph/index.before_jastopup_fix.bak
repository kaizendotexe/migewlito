    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const allLanguageCards = document.querySelectorAll(".language-card");
            const countryRadios = document.querySelectorAll(".radio-country");
            const languageRadios = document.querySelectorAll(".radio-language");
            const saveBtn = document.getElementById("saveCountry");
        
            const urlMap = {
                "ID-id": "https://gogogo.id",
                "MY-en": "https://gogogo.com/en-my",
                "PH-en": "https://gogogo.com/en-ph",
            };
        
            const modalCountry = document.getElementById("modalCountry");
            modalCountry.addEventListener("show.bs.modal", function () {
                allLanguageCards.forEach(card => card.style.display = "none");
                languageRadios.forEach(radio => radio.checked = false);
                countryRadios.forEach(radio => radio.checked = false);
            });
        
            countryRadios.forEach(radio => {
                radio.addEventListener("change", function () {
                    const selectedCountry = this.id.split("-")[1];
                    allLanguageCards.forEach(card => {
                        const langRadio = card.querySelector("input");
                        if (langRadio && langRadio.id.includes(`language-${selectedCountry}`)) {
                            card.style.display = "block";
                        } else {
                            card.style.display = "none";
                        }
                    });
                });
            });
        
            saveBtn.addEventListener("click", function () {
                const selectedCountry = document.querySelector(".radio-country:checked");
                const selectedLanguage = document.querySelector(".radio-language:checked");
        
                if (!selectedCountry && !selectedLanguage) {
                    Swal.fire({
                        icon: 'warning',
                        title: 'Choose Region & Language!',
                        text: 'Please select region and language first.',
                        confirmButtonText: 'OK',
                        confirmButtonColor: "var(--warna_3)",
                        background: '#333333',
                        color: 'white',
                    });
                    return;
                }
        
                if (!selectedLanguage) {
                    Swal.fire({
                        icon: 'warning',
                        title: 'Select Language!',
                        text: 'Please select a language first.',
                        confirmButtonText: 'OK',
                        confirmButtonColor: "var(--warna_3)",
                        background: '#333333',
                        color: 'white',
                    });
                    return;
                }
        
                const countryCode = selectedCountry.id.split("-")[1];
                const languageCode = selectedLanguage.id.split("-")[2];
                const mapKey = `${countryCode}-${languageCode}`;
                const redirectURL = urlMap[mapKey];
        
                if (redirectURL) {
                    window.location.href = redirectURL;
                } else {
                    Swal.fire({
                        icon: 'warning',
                        title: 'URL not found!',
                        text: 'URL for this region and language are not yet available.',
                        confirmButtonText: 'OK',
                        confirmButtonColor: "var(--warna_3)",
                        background: '#333333',
                        color: 'white',
                    });
                }
            });
        });
    </script>
    
