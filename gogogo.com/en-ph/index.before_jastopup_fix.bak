<script>
    document.addEventListener('DOMContentLoaded', function () {
        // Menangkap semua elemen dengan kelas 'nav-link'
        var navLinks = document.querySelectorAll('.iniaja');
    
        // Iterasi melalui setiap elemen
        navLinks.forEach(function (link) {
            // Menambahkan event listener untuk setiap elemen
            link.addEventListener('click', function (e) {
                // Mencegah perilaku bawaan href
                e.preventDefault();

                // Menghapus kelas active dari semua nav-link
                navLinks.forEach(function (link) {
                    link.classList.remove('active');
                });

                // Menambahkan kelas active pada elemen yang diklik
                this.classList.add('active');
    
                // Mendapatkan target dari atribut 'href'
                var targetId = this.getAttribute('href').substring(1);
    
                // Mendapatkan elemen target berdasarkan ID
                var targetElement = document.getElementById(targetId);
    
                // Mendapatkan offset (jarak) dari elemen target ke atas viewport
                var offset = targetElement.getBoundingClientRect().top - 170; // Menambahkan offset sebesar 170 piksel
    
                // Scroll smooth ke elemen target dengan offset
                if (targetElement) {
                    window.scrollTo({
                        top: window.scrollY + offset,
                        behavior: 'smooth'
                    });
                }
            });
        });
    });
</script>

