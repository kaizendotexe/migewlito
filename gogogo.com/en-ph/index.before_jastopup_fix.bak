<script>
function updateButtonVisibility(category, totalGames) {
    var offset = $('div[name="game-' + category + '"]').length;
    var $showLessBtn = $('.show-less-btn[data-category="' + category + '"]');
    var $loadMoreBtn = $('.load-more-btn[data-category="' + category + '"]');
    $showLessBtn.toggle(offset > 10);
    $loadMoreBtn.toggle(offset < totalGames);
}
$('.load-more-btn').click(function () {
    var $button = $(this);
    var category = $button.data('category');
    var offset = $('div[name="game-' + category + '"]').length; 
    var totalGames = $button.data('count'); 
    $button.prop('disabled', true).html('<span class="spinner-border spinner-border-sm pr-1" role="status"></span>Loading...');
    $.post("home/load_more_games.html", { category, offset }, function (response) {
        $('.games-container[data-category="' + category + '"]').append(response);
        updateButtonVisibility(category, totalGames);
    }).always(function () {
        $button.prop('disabled', false).html('Show More' +
            '<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">' +
            '<path d="M17 9.46875L12 14.4688L7 9.46875" stroke="var(--warna_3)" stroke-width="2.5" stroke-linecap="round" stroke-linejoin="round"/>' +
            '</svg>');
    });
});
$('.show-less-btn').click(function () {
    var $button = $(this);
    var category = $button.data('category');
    var offset = 0;
    $button.prop('disabled', true).html('<span class="spinner-border spinner-border-sm pr-1" role="status"></span>Loading...');
    $.post("home/show_less_games.html", { category, offset }, function (response) {
        $('.games-container[data-category="' + category + '"]').empty().append(response);
        var totalGames = $('.load-more-btn[data-category="' + category + '"]').data('count');
        updateButtonVisibility(category, totalGames);
    }).always(function () {
        $button.prop('disabled', false).html('Show Less' +
            '<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">' +
            '<path stroke="var(--warna_3)" stroke-linecap="round" stroke-linejoin="round" stroke-width="2.5" d="m17 14l-5-5l-5 5"/>' +
            '</svg>');
    });
});
$(document).ready(function () {
    $('.load-more-btn').each(function () {
        var category = $(this).data('category');
        var totalGames = $(this).data('count');
        updateButtonVisibility(category, totalGames);
    });
});
</script>
